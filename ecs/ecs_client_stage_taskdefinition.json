{
    "containerDefinitions": [
        {
            "name": "client",
            "image": "%s.dkr.ecr.us-east-1.amazonaws.com/rwk/test-driven-client:staging",
            "essential": true,
            "memoryReservation": 300,
            "portMappings": [
                {
                    "hostPort": 0,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "environment": [
                {
                    "name": "REACT_APP_API_GATEWAY_URL",
                    "value": "https://rdok4ehqce.execute-api.us-east-1.amazonaws.com/v1/execute"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "testdriven-client-stage",
                    "awslogs-region": "us-east-1"
                }
            }
        }
    ],
    "family": "testdriven-client-stage-td"
}